    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "You see the shadowy figures inching closer and closer. \nThey are on your
      bed, \nthey are behind your curtains, \nthey stand atop your desk."
  - portrait: {fileID: 0}
    character: {fileID: 0}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "You struggle like your life depends on it. \nYet, \nyour body remains
      in place."

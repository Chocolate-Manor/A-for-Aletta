    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "You feel like you are floating. \nYour heart beats like drumsticks. \nYou
      struggle to move."
  - portrait: {fileID: 21300000, guid: dde12b3fccea7bf4681b1630209daa48, type: 3}
    character: {fileID: 11400000, guid: 62c2e9ac0ef521b4ca32d599917c1db2, type: 2}
    nameOverwrite: Shade
    hasNameOverwrite: 1

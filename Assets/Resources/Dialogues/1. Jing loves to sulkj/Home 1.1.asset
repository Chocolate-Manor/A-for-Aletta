  m_Name: Home 1.1
  m_EditorClassIdentifier: 
  sceneHeading: INT. HOME - NIGHT
  backgroundMusic: {fileID: 8300000, guid: d78f97f048fcf184bbb0861150f3ee45, type: 3}
  hasWhiteTransition: 0

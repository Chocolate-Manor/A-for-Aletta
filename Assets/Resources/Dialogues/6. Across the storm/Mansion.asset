  m_Name: Mansion
  m_EditorClassIdentifier: 
  sceneHeading: EXT. THIRTY-FIRST - DAY
  backgroundMusic: {fileID: 8300000, guid: 9b9ca48c7d52e3d40aab0561d3a5f4d1, type: 3}
  hasWhiteTransition: 0
  bloomOn: 1
  hasEnterSound: 0
  enterSound: {fileID: 0}
  hasExitSound: 0
  exitSound: {fileID: 0}

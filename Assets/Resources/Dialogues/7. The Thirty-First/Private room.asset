  m_Name: Private room
  m_EditorClassIdentifier: 
  sceneHeading: INT. PRIVATE ROOM - DAY
  backgroundMusic: {fileID: 8300000, guid: e3d66da02465e54498981b61d0bdfdce, type: 3}

    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "You pull her down, \nfeet touching the floor. \nShe still floats, \nlike

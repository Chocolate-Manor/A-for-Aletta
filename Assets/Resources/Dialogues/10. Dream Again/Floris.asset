    screenShake: 1
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: '!?'
  - portrait: {fileID: 21300000, guid: 5ea3c183a38190347a8b4cee4d994364, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "You were here? \nThe whole time?"
  - portrait: {fileID: 0}
    character: {fileID: 0}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "You release your hand. \nThe bricks fall back into place. "
  - portrait: {fileID: 21300000, guid: bf52198023c3e8d44a758fac06b28d51, type: 3}
    character: {fileID: 11400000, guid: ff817fee81eabdc4ea6617087dd5008e, type: 2}

    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: And she almost killed you.
  - portrait: {fileID: 21300000, guid: bbe47528a5511bd439a3bf8950032f8e, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}

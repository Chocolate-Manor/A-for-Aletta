  m_EditorHideFlags: 0
  m_Script: {fileID: 11500000, guid: 23696f8c916c30f40bdd399459ac4043, type: 3}
  m_Name: Reunion 1.2
  m_EditorClassIdentifier: 
  sceneHeading: EXT. THE CORRIDOR - DAY
  backgroundMusic: {fileID: 8300000, guid: bfd75b5ae8a39644895889348b8b800c, type: 3}
  hasWhiteTransition: 1
  bloomOn: 1
  dialogueLines:
  - portrait: {fileID: 0}
    character: {fileID: 0}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "The running figure had stopped. \nYou realize it's a figure of a girl.
      \nGreen checkered skirt, \nneat white blouse, \nflat bangs that stops right
      above the eye."
  - portrait: {fileID: 21300000, guid: 80dab4d17dc468c42b8a5c5b1b2e2b15, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.1
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: 'Z-

      Zhu...?'
  - portrait: {fileID: 0}
    character: {fileID: 0}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: You suddenly find your throat closing up.
  - portrait: {fileID: 21300000, guid: d5a6df087a914ed4a9eebddf39482f9e, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: Hey.
  - portrait: {fileID: 21300000, guid: d5a6df087a914ed4a9eebddf39482f9e, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "She says. \nYou can't read her expression."
  - portrait: {fileID: 21300000, guid: 91c25d7b9a1ff604c8b1e09ec2be1221, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: 'H-

      ey.'
  - portrait: {fileID: 0}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: You answer.
  - portrait: {fileID: 21300000, guid: 91c25d7b9a1ff604c8b1e09ec2be1221, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: 'It''s been...

      a while.'
  - portrait: {fileID: 21300000, guid: d5a6df087a914ed4a9eebddf39482f9e, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: Um.
  - portrait: {fileID: 0}
    character: {fileID: 0}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: She nods.
  - portrait: {fileID: 0}
    character: {fileID: 0}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "You step forward. \nShe steps back."
  - portrait: {fileID: 21300000, guid: d5a6df087a914ed4a9eebddf39482f9e, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: Why?
  - portrait: {fileID: 0}
    character: {fileID: 0}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "She questions. \nHer voice weak."
  - portrait: {fileID: 21300000, guid: 80dab4d17dc468c42b8a5c5b1b2e2b15, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: I...
  - portrait: {fileID: 0}
    character: {fileID: 0}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: You can't find the words.
  - portrait: {fileID: 21300000, guid: d5a6df087a914ed4a9eebddf39482f9e, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "I thought I would die... \nBut I didn't."
  - portrait: {fileID: 21300000, guid: d5a6df087a914ed4a9eebddf39482f9e, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "I believed you... \nWhat an idiot I was."
  - portrait: {fileID: 21300000, guid: d5a6df087a914ed4a9eebddf39482f9e, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: Why is it always like this?
  - portrait: {fileID: 21300000, guid: 91c25d7b9a1ff604c8b1e09ec2be1221, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: ...?
  - portrait: {fileID: 0}
    character: {fileID: 0}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "You focus your gaze. \nYou see her eyes muddled with tears."
  - portrait: {fileID: 21300000, guid: 80dab4d17dc468c42b8a5c5b1b2e2b15, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: 'What are you...

      talking about?'
  - portrait: {fileID: 21300000, guid: dae3d178c2cbafc48894ce82877fee70, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: What am I talking about?
  - portrait: {fileID: 21300000, guid: dae3d178c2cbafc48894ce82877fee70, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: You sense a hint of anger in her voice.
  - portrait: {fileID: 21300000, guid: 5c0748f0de081584cbb396b156d7063b, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: You promised me.
  - portrait: {fileID: 21300000, guid: 5c0748f0de081584cbb396b156d7063b, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: You promised me we'd be together.
  - portrait: {fileID: 0}
    character: {fileID: 0}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "She no longer sounds unsure. \nYou find her in your face, \nher hand on
      your collar."
  - portrait: {fileID: 21300000, guid: dae3d178c2cbafc48894ce82877fee70, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 31
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "You said it, \ndidn't you?"
  - portrait: {fileID: 21300000, guid: 5c0748f0de081584cbb396b156d7063b, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 32
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "You looked at me in the eye and said it, \ndidn't you?!"
  - portrait: {fileID: 21300000, guid: 5c0748f0de081584cbb396b156d7063b, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 35
    typeSpeed: 0.05
    screenShake: 1
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: Then how could you!
  - portrait: {fileID: 0}
    character: {fileID: 0}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: You find yourself unable to think.
  - portrait: {fileID: 21300000, guid: 5c0748f0de081584cbb396b156d7063b, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 35
    typeSpeed: 0.02
    screenShake: 1
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "No letters. \nNever any letters. \nEvery time the nurse comes in with
      a Trolley my heart would drop. \nThen I'd just be left disappointed."
  - portrait: {fileID: 21300000, guid: 80dab4d17dc468c42b8a5c5b1b2e2b15, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 35
    typeSpeed: 0.02
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "What? \nNurse? \nYou were sick? \nI-"
  - portrait: {fileID: 21300000, guid: 5c0748f0de081584cbb396b156d7063b, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 35
    typeSpeed: 0.02
    screenShake: 1
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "I gave everything to you, \nand you can't even put some ink on a paper?"
  - portrait: {fileID: 21300000, guid: 57e530106eac9a34cbb17b1d1b165e49, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 35
    typeSpeed: 0.02
    screenShake: 1
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "I did! \nI wrote to you! \nYou never replied so I thought-"
  - portrait: {fileID: 21300000, guid: 5c0748f0de081584cbb396b156d7063b, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 35
    typeSpeed: 0.02
    screenShake: 1
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "No goodbye. \nNot even a goodbye. \nJust gone,\nlike a gust of wind. \nDon't
      I at least deserve that much?!"
  - portrait: {fileID: 21300000, guid: de8e10d247fbc1e4398a841522ab9dbd, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 35
    typeSpeed: 0.02
    screenShake: 1
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "I did! \nI wrote you goodbye! \nI put it in your book, \nthe book you
      borrowed me! \nI left it on the bench-"
  - portrait: {fileID: 21300000, guid: de8e10d247fbc1e4398a841522ab9dbd, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 35
    typeSpeed: 0.02
    screenShake: 1
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "I saw you picking it up, \nI-"
  - portrait: {fileID: 21300000, guid: 592567230f0c3a2458b56f2ea4b23cee, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.1
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: You were there?
  - portrait: {fileID: 0}
    character: {fileID: 0}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: Her tone changes.
  - portrait: {fileID: 21300000, guid: de8e10d247fbc1e4398a841522ab9dbd, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: I-
  - portrait: {fileID: 21300000, guid: 8e4c9c9e15be0ad46bc7521d4ae451cb, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.1
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: 'You were there. '
  - portrait: {fileID: 0}
    character: {fileID: 0}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "She starts to sound hoarse. \nA stingy feeling courses through your body.
      \nSomething thin and dark seems to grow out the edge of her eyes."
  - portrait: {fileID: 21300000, guid: 8e4c9c9e15be0ad46bc7521d4ae451cb, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.03
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "You played her. \nYou hid out of sight, \njust to see how she would react.
      \nTo see if you matter."
  - portrait: {fileID: 21300000, guid: 5ffe8b7f2c9d3ba48a1ef46f90dc6552, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 35
    typeSpeed: 0.02
    screenShake: 1
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "No! \nWha-"
  - portrait: {fileID: 21300000, guid: 8e4c9c9e15be0ad46bc7521d4ae451cb, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 32
    typeSpeed: 0.02
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "You wanted me to hurt. \nBecause only then can you be sure that I care."
  - portrait: {fileID: 21300000, guid: 5ea3c183a38190347a8b4cee4d994364, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 35
    typeSpeed: 0.02
    screenShake: 1
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "Huh?! \nThat's n-"
  - portrait: {fileID: 21300000, guid: 5c0748f0de081584cbb396b156d7063b, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 35
    typeSpeed: 0.01
    screenShake: 1
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: 'Because that''s what you are after. '
  - portrait: {fileID: 21300000, guid: 5c0748f0de081584cbb396b156d7063b, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 35
    typeSpeed: 0.01
    screenShake: 1
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: 'To feel important. '
  - portrait: {fileID: 21300000, guid: 5c0748f0de081584cbb396b156d7063b, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 35
    typeSpeed: 0.01
    screenShake: 1
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "To feel that you have gotten the upper hand. \n"
  - portrait: {fileID: 21300000, guid: 5c0748f0de081584cbb396b156d7063b, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 35
    typeSpeed: 0.01
    screenShake: 1
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: 'So that I''d feel desperate. '
  - portrait: {fileID: 21300000, guid: 5c0748f0de081584cbb396b156d7063b, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 35
    typeSpeed: 0.01
    screenShake: 1
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: So I'd cling onto you like I never would have before-
  - portrait: {fileID: 21300000, guid: bcca14618ab11cf42942f7c064d5bf87, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 35
    typeSpeed: 0.01
    screenShake: 1
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: NO!!
  - portrait: {fileID: 21300000, guid: bcca14618ab11cf42942f7c064d5bf87, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 35
    typeSpeed: 0.01
    screenShake: 1
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: WHAT!?
  - portrait: {fileID: 21300000, guid: f05c7d4138407c84c9b8921efd7123c6, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 35
    typeSpeed: 0.01
    screenShake: 1
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: NO!!!!!!!!
  - portrait: {fileID: 0}
    character: {fileID: 0}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: You find yourself roaring like a wild animal.
  - portrait: {fileID: 21300000, guid: bcca14618ab11cf42942f7c064d5bf87, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 35
    typeSpeed: 0.01
    screenShake: 1
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "That's not what happened at all! \nHow could you even?! \nI would never
      do a thing like that, \nand you know whole heartily that I won't!"
  - portrait: {fileID: 21300000, guid: d8118b863fb22214a9bca9902324042e, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "That's ridiculous. \nThat's...\nI would never..."
  - portrait: {fileID: 21300000, guid: a2aa609260ead4d44b8a13037e8aca89, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "What could I have done?! \nMy mother was crying, \nI..."
  - portrait: {fileID: 0}
    character: {fileID: 0}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "Zhu pauses. \nYour roar seemed to have shocked her. \nShe quiets down.
      \r\n"
  - portrait: {fileID: 21300000, guid: 16ddd8120eb26ae4db4b14bf59250a8f, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: ...
  - portrait: {fileID: 0}
    character: {fileID: 0}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "She lowers her head, \nhand on her forehead. \nYou could no longer see
      the dark threads."
  - portrait: {fileID: 21300000, guid: 16ddd8120eb26ae4db4b14bf59250a8f, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: ...Sorry, I...
  - portrait: {fileID: 0}
    character: {fileID: 0}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: You find your anger evaporate.
  - portrait: {fileID: 21300000, guid: 7320f99f9377cb24b9ffdc250ef920d9, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "Well. \nyou are not wrong."
  - portrait: {fileID: 21300000, guid: e798b3da780407a4a9a113d2c8f2fdef, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: 'It was...

      my fault.'
  - portrait: {fileID: 21300000, guid: 069a71f29937e3b4c9cb41bdb52cde3b, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: If only I could have had the courage.
  - portrait: {fileID: 21300000, guid: 7320f99f9377cb24b9ffdc250ef920d9, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "But I couldn't. \nI was a coward. \nI was too afraid what my words might
      do."
  - portrait: {fileID: 0}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: She is still looking down at the ground.
  - portrait: {fileID: 21300000, guid: 7224964e6ddc5ac44ac6ac0fa985c615, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.07
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: Then don't leave...
  - portrait: {fileID: 0}
    character: {fileID: 0}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: She whispers.
  - portrait: {fileID: 21300000, guid: 7224964e6ddc5ac44ac6ac0fa985c615, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.07
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: ...Then stay.
  - portrait: {fileID: 21300000, guid: e7c001e0d2b7a8a4eb705ce5928dda27, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.07
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: Don't you get it?
  - portrait: {fileID: 0}
    character: {fileID: 0}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: You bite your lip.
  - portrait: {fileID: 21300000, guid: cc249a7f12e72bf47a580efa89c2be67, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.07
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: I wish I could.
  - portrait: {fileID: 21300000, guid: 16ddd8120eb26ae4db4b14bf59250a8f, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: Then make it up to me.
  - portrait: {fileID: 21300000, guid: e7c001e0d2b7a8a4eb705ce5928dda27, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.07
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: ...How?
  - portrait: {fileID: 0}
    character: {fileID: 0}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "You stare at her, \nsurprised."
  - portrait: {fileID: 21300000, guid: b3bbacd510577e244970070f5ddd49d9, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "I am in front of you, \naren't I?"
  - portrait: {fileID: 0}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: She raises her head with a forced smile.
  - portrait: {fileID: 21300000, guid: 5ea3c183a38190347a8b4cee4d994364, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: 'But this is just...

      you are not-'
  - portrait: {fileID: 21300000, guid: b3bbacd510577e244970070f5ddd49d9, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: -Real?
  - portrait: {fileID: 21300000, guid: b3bbacd510577e244970070f5ddd49d9, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: She finishes your sentence.
  - portrait: {fileID: 21300000, guid: 5ea3c183a38190347a8b4cee4d994364, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: ...real.
  - portrait: {fileID: 21300000, guid: a540f42f289a07a42bfaca9faaa3e59c, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "She put her hand behind her back, \nhead slightly tilt. \nShe brushes
      her fringes to the side like you had saw her do a million times.\r\n"
  - portrait: {fileID: 21300000, guid: a540f42f289a07a42bfaca9faaa3e59c, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "But I am. \nAs real as real could be."
  - portrait: {fileID: 21300000, guid: 5ea3c183a38190347a8b4cee4d994364, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "That's...\nnot... \nEven if there's way to visit someone in a dream..."
  - portrait: {fileID: 21300000, guid: a540f42f289a07a42bfaca9faaa3e59c, type: 3}
    character: {fileID: 11400000, guid: 19c5cb255516aac438c9ac99f1001c5d, type: 2}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}

    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: You are amazed that you could make the game at all.
  - portrait: {fileID: 21300000, guid: eb5b088065ec9ec43b31eb8ba35508b9, type: 3}
    character: {fileID: 11400000, guid: 87b413933eb845b41b53059890fa7feb, type: 2}

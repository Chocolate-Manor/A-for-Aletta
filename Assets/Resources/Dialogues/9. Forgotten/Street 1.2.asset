    hasNameOverwrite: 1
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 32
    typeSpeed: 0.04
    screenShake: 1
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "You can't break me, \nyou squint-eyed chink!"
  - portrait: {fileID: 0}

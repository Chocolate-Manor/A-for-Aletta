    textSize: 33
    typeSpeed: 0.04
    screenShake: 1
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "Hey! \nChink! \nWhat do you think you are doing! \nI could have died!"
  - portrait: {fileID: 21300000, guid: 069a71f29937e3b4c9cb41bdb52cde3b, type: 3}

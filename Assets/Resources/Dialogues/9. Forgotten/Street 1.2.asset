    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "You turn around, \nlooking for the lamb. \nHowever, \nit is nowhere to
      be found, \nlike it had disappeared in a puff of smoke. "
  - portrait: {fileID: 21300000, guid: 76c75b69e36488744a8854d1f0a97532, type: 3}

    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "You turn back towards the house for a good look. \nIt's old, \nbut fairly
      well maintained. \nIt is surrounded by tall stone fences, \npreventing you
      from peeking inside. "
  - portrait: {fileID: 0}
    character: {fileID: 0}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "The name plate has the name \"Heath & xxx\". \nThe second part is forcefully
      scratched out. \nYou find the name Heath oddly familiar..."
  - portrait: {fileID: 0}
    character: {fileID: 0}
    nameOverwrite: 
    hasNameOverwrite: 0
    textColor: {r: 0.5058824, g: 0.5058824, b: 0.5058824, a: 1}
    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 1
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0

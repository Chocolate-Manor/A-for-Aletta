    textSize: 30
    typeSpeed: 0.05
    screenShake: 0
    isNarration: 0
    musicToSwitchTo: {fileID: 0}
    switchMusic: 0
    text: "No, \nNono. \nHaha, \nmy bad. \nIt would seem your eyes are not so small
